Package Ashyi.ashyiView;

import Ashyi.domainModel.*;
import Ashyi.contentView.*;
import Ashyi.presentationView.*;
import Ashyi.navigationView.*;
import Primitives.*;

function ashyi(in Student student, in Teacher teacher, in SystemManager systemManager, in SystemContext context){
	List<T> adaptedList;
	Page page;
	NavigationModel navigationModel;
	
	adaptedList <- content(student, teacher, systemManager, context); 
	page <- presentation(adaptedList);	
	navigationModel <- navigation(page);
}

